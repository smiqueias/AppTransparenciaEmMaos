<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#004D40"
    android:layout_height="match_parent"
    tools:context=".general.ui.TelaPrincipal">

    <View
        android:id="@+id/view_gastosBolsaFamilia"
        android:layout_width="152dp"
        android:layout_height="129dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/shape_main_activity"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </View>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="67dp"
        android:layout_height="73dp"
        app:layout_constraintBottom_toBottomOf="@+id/view_gastosBolsaFamilia"
        app:layout_constraintEnd_toEndOf="@+id/view_gastosBolsaFamilia"
        app:layout_constraintStart_toStartOf="@+id/view_gastosBolsaFamilia"
        app:layout_constraintTop_toTopOf="@+id/view_gastosBolsaFamilia"
        app:layout_constraintVertical_bias="0.17000002"
        app:srcCompat="@drawable/abc_vector_test" />

    <TextView
        android:id="@+id/label_consultaBolsaFamilia"
        android:layout_width="139dp"
        android:layout_height="28dp"
        android:layout_marginTop="8dp"
        android:text="@string/gastos_bolsa_fam_lia"
        android:textAlignment="center"
        android:textSize="14sp"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        android:fontFamily="sans-serif-medium"
        app:layout_constraintEnd_toEndOf="@+id/view_gastosBolsaFamilia"
        app:layout_constraintStart_toStartOf="@+id/view_gastosBolsaFamilia"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

</androidx.constraintlayout.widget.ConstraintLayout>